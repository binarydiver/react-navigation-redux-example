{
  "presets": ["babel-preset-expo"],
  "env": {
    "development": {
      "plugins": ["transform-react-jsx-source"]
    }
  },
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "@app": "./app",
          "@action": "./app/action",
          "@reducer": "./app/reducer",
          "@screen": "./app/screen"
        }
      }
    ]
  ]
}
